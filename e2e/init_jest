const detox = require('detox');
const config = require('../package.json').detox;

jasmine.DEFAULT_TIMEOUT_INTERVAL = 500000

beforeAll(async () => {
  console.log("-----------> before detox.init")
  try {
    await detox.init(config)
  } catch (e) {
    console.log(e)
  }
  console.log("-----------> after detox.init")
});

afterAll(async () => {
  await detox.cleanup();
});
